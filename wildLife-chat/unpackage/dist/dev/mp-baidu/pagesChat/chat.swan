<view class="data-v-14d0b84e"><view class="content data-v-14d0b84e" style="{{'height:'+(chatHeight+'px')+';'}}"><view class="header data-v-14d0b84e"><view class="name data-v-14d0b84e">{{animalData.nickname}}</view><view class="sign data-v-14d0b84e">{{animalData.sign}}</view></view><scroll-view class="msg-list data-v-14d0b84e" style="{{'height:'+(chatHeight+'px')+';'}}" scroll-y="true" scroll-top="{{scrollTop}}" id="scrollview"><block s-for="{{chatContent}}" s-for-item="item" s-for-index="index" s-key="index"><view id="msgbox" class="data-v-14d0b84e"><view class="{{['msgbox data-v-14d0b84e',item.openid==userData.openid?'userMsg':'']}}"><view class="avater data-v-14d0b84e"><image class="img data-v-14d0b84e" src="{{item.headimgurl}}" mode="aspectFill"></image></view><view class="msgtext data-v-14d0b84e"><view class="text data-v-14d0b84e">{{item.chatMsg}}</view></view></view></view></block></scroll-view></view><view class="footer data-v-14d0b84e" style="{{'bottom:'+(inputBottom+'px')+';'}}"><view data-event-opts="{{[['tap',[['showChatBox',['$event']]]]]}}" class="inputRoom data-v-14d0b84e" bindtap="__e">{{textMsg}}</view><button data-event-opts="{{[['tap',[['send',['$event']]]]]}}" class="send-btn data-v-14d0b84e" bindtap="__e">发送</button></view><block s-if="{{chatBoxShow==true}}"><view class="chatbox data-v-14d0b84e"><block s-for="{{userChatlist}}" s-for-item="list" s-for-index="listIndex" s-key="listIndex"><view data-event-opts="{{[['tap',[['selectChat',['$0'],[[['userChatlist','',listIndex,'keyword']]]]]]]}}" class="userChatlist data-v-14d0b84e" bindtap="__e"><view class="text data-v-14d0b84e">{{list.keyword}}</view></view></block></view></block></view>
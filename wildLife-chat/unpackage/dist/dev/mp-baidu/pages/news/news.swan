<view class="container" style="{{'width:'+(Width*2+'rpx')+';'}}"><view class="userData"><view class="avater"><image class="img" src="{{userData.headimgurl}}" mode="aspectFill"></image></view><view class="nickname">{{''+userData.nickname+''}}</view></view><view class="search"><uni-search-bar vue-id="1cdb3631-1" placeholder="搜索" cancelButton="none" bgColor="#EAECEC" radius="0" data-event-opts="{{[['^input',[['search']]]]}}" bind:input="__e"></uni-search-bar></view><view class="newsBox" style="{{'height:'+(Heigh*2-400+'rpx')+';'}}"><scroll-view class="scroll-Y" style="{{'height:'+(Heigh*2-400+'rpx')+';'}}" scroll-y="true"><block s-for="{{chatRoomList}}" s-for-item="item" s-for-index="index" s-key="index"><view class="newsList"><view data-event-opts="{{[['tap',[['toMsg',['$0'],[[['chatRoomList','',index,'animalopenid']]]]]]]}}" class="{{['avater',(item.isdie==0)?'lineFriends':'']}}" bindtap="__e"><image class="img" src="{{item.headimgurl}}" mode="aspectFill"></image><view class="doc"></view></view><view data-event-opts="{{[['tap',[['toChat',['$0'],[[['chatRoomList','',index]]]]]]]}}" class="content" bindtap="__e"><view class="name">{{item.nickname}}</view><view class="news">{{item.newMsg}}</view></view></view></block></scroll-view></view></view>
<view class="container" style="{{'width:'+(Width*2+'rpx')+';'}}"><view class="userData"><view class="avater"><image class="img" src="{{userData.headimgurl}}" mode="aspectFill"></image></view><view class="nickname">{{''+userData.nickname+''}}</view></view><view class="entry"><view data-event-opts="{{[['tap',[['onLineFriends',['$event']]]]]}}" class="left btn" bindtap="__e"><view class="{{[(leftStyle==true)?'clickStyle':'']}}">在线好友</view></view><view data-event-opts="{{[['tap',[['lineFriends',['$event']]]]]}}" class="right btn" bindtap="__e"><view class="{{[(rightStyle==true)?'clickStyle':'']}}">离线好友</view></view></view><view class="search"><uni-search-bar vue-id="08da3b36-1" placeholder="搜索" cancelButton="none" bgColor="#EAECEC" radius="0" data-event-opts="{{[['^input',[['search']]]]}}" bind:input="__e"></uni-search-bar></view><block s-if="{{leftStyle==true}}"><view class="onLineFriends" style="{{'height:'+(Heigh*2-480+'rpx')+';'}}"><scroll-view class="scroll-Y" style="{{'height:'+(Heigh*2-400+'rpx')+';'}}" scroll-y="true"><block s-for="{{onlineData}}" s-for-item="item1" s-for-index="index1" s-key="index1"><view class="friendList"><view data-event-opts="{{[['tap',[['toMsg',['$0'],[[['onlineData','',index1,'openid']]]]]]]}}" class="avater" bindtap="__e"><image class="img" src="{{item1.headimgurl}}"></image><view class="doc"></view></view><view data-event-opts="{{[['tap',[['toChat',['$0'],[[['onlineData','',index1]]]]]]]}}" class="content" bindtap="__e"><view class="name">{{item1.nickname}}</view><view class="sign">{{item1.sign}}</view></view></view></block></scroll-view></view></block><block s-if="{{rightStyle==true}}"><view class="lineFriends" style="{{'height:'+(Heigh*2-480+'rpx')+';'}}"><scroll-view class="scroll-Y" style="{{'height:'+(Heigh*2-400+'rpx')+';'}}" scroll-y="true"><block s-for="{{lineData}}" s-for-item="item2" s-for-index="index2" s-key="index2"><view class="friendList"><view data-event-opts="{{[['tap',[['toMsg',['$0'],[[['lineData','',index2,'openid']]]]]]]}}" class="avater" bindtap="__e"><image class="img" src="{{item2.headimgurl}}"></image><view class="doc"></view></view><view data-event-opts="{{[['tap',[['toChat',['$0'],[[['lineData','',index2]]]]]]]}}" class="content" bindtap="__e"><view class="name">{{item2.nickname}}</view><view class="sign">{{item2.sign}}</view></view></view></block></scroll-view></view></block></view>